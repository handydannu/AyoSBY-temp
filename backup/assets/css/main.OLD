}

.scroll-to-top {
	display: inline-block;
	position: fixed;
	padding: 7px 7px 7px 9px;
	width: 45px;
	height: 45px;
	/*background: url('../img/bg-trans-4.png') repeat;*/ /* for IE compatibility */
	background: #EA0028;
	font-size: 22px;
	color: #FFF;
	text-align: center;
	right: 5%;
	bottom: 85px;
	z-index: 3;
	outline: none !important;
