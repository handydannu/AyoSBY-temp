#nav-mainmenu {
	min-height: 450px; /* can be inherited from #container */
	position: absolute;
	right: 0;
	z-index: 99999;
	height: inherit;
}

.nav-mainmenu ul {
	padding: 0;
	margin: 0;
	list-style: none;
	width: 350px;
	height: inherit; /* chrome: 526, firefox: 515, ie: 441; */
	background: #085b59;
	overflow-x: hidden;
	/*overflow-y: scroll;*/
}

.nav-mainmenu > ul {
    height: 460px;
}

.nav-mainmenu ul li {
	width: inherit;
}

.nav-mainmenu ul li a.nav-primary,
.nav-mainmenu ul li .nav-child li a {
	width: inherit;
	display: inline-block;
	padding: 12px 20px 12px;
	color: #FFF;
	background: transparent;
	text-decoration: none;
	border-bottom: 1px solid #084d59;
	border-top: 1px solid #0a7774;
}

.nav-mainmenu ul li a.nav-primary:hover,
.nav-mainmenu ul li .nav-child li a:hover {
	background: #0b7a78;
}

.nav-mainmenu ul li a.nav-parent,
.nav-mainmenu ul li a.nav-close {
	float: left;
	display: block;
	width: 305px;
	margin: 0;
	border-right: 1px solid #084d59;
}

.nav-mainmenu a.nav-child-chevron,
.nav-mainmenu a.nav-close-chevron {
	float: left;
	display: block;
	width: 45px;
	padding: 12px 15px 12px;
	margin: 0;
	color: #CFCFCF;
	border-bottom: 1px solid #084d59;
	border-top: 1px solid #0a7774;
	border-left: 1px solid #0a7774;
}

.nav-mainmenu a.nav-child-chevron:hover,
.nav-mainmenu a.nav-close-chevron:hover {
	background: #0A7170;
	color: #FFF;
	cursor: pointer;
}

.nav-mainmenu a.nav-close-chevron:hover {
	background: #992316;
}

.nav-mainmenu .nav-child {
	float: left;
	background: #0a6866;
	border-bottom: 1px solid #096866;
	overflow: hidden;
}

